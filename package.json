{
  "name": "observable-tree-router",
  "version": "0.2.0",
  "description": "Type-safe, state-oriented, reactive, hierarchical router for single page applications",
  "keywords": [
    "router",
    "typescript",
    "rxjs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/couzic/observable-tree-router.git"
  },
  "homepage": "https://github.com/couzic/observable-tree-router#readme",
  "bugs": {
    "url": "https://github.com/couzic/observable-tree-router/issues"
  },
  "author": {
    "name": "Mikael Couzic",
    "email": "mikaelcouzic@gmail.com",
    "url": "https://github.com/couzic"
  },
  "scripts": {
    "prebuild": "npm run test && shx rm -rf ./lib",
    "build": "tsc",
    "prepare": "npm run build",
    "doctoc": "doctoc . --notitle --maxlevel 4",
    "test": "mocha && npm run acceptance-test && npm run test-compilation-errors",
    "acceptance-test": "echo no acceptance tests",
    "test-compilation-errors": "node ./testCompilationErrors.js",
    "tdd": "mocha --watch",
    "lint": "tslint --project ./ --fix"
  },
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "mocha": {
    "require": "ts-node/register",
    "extension": [
      "ts"
    ],
    "spec": [
      "src/**/*.test.ts"
    ],
    "reporter": "dot"
  },
  "peerDependencies": {
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/ramda": "^0.28.9",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "chai": "^4.3.6",
    "chalk": "^5.0.1",
    "doctoc": "^2.1.0",
    "glob-promise": "^4.2.2",
    "history": "^5.3.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "ramda": "^0.28.0",
    "rxjs": "^7.5.5",
    "rxjs-tslint": "^0.1.8",
    "shx": "^0.3.4",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "path-parser": "^6.1.0"
  }
}
